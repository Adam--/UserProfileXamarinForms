<?xml version="1.0" encoding="UTF-8" ?>
<ResourceDictionary
    x:Class="UserProfileXamarinForms.Theme.DarkTheme"
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">

    <Color x:Key="NavigationBarColor">#FFFFFF</Color>
    <Color x:Key="PageBackgroundColor">#202020</Color>

    <Color x:Key="PrimaryColor">#000000</Color>
    <Color x:Key="SecondaryColor">#FFFFFF</Color>

    <Color x:Key="PrimaryTextColor">#CCCCCC</Color>
    <Color x:Key="SecondaryTextColor">#949494</Color>

    <Color x:Key="ImageOverlayColor">#000000</Color>
    <Color x:Key="ImageOverlayTextColor">#FFFFFF</Color>

    <x:String x:Key="MessageIcon">message_icon_dark</x:String>
    <x:String x:Key="GridIcon">grid_icon_dark</x:String>
</ResourceDictionary>